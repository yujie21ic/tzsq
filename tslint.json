{
    "rules": {
        "______": "const 统一表示 引用不可变的意思",
        "triple-equals": true,
        "no-var-keyword": true,
        "no-conditional-assignment": true,
        "no-default-export": true,
        "only-arrow-functions": [
            true
        ],
        "arrow-return-shorthand": [
            true,
            "multiline"
        ],
        "no-invalid-this": true,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ]
    }
}